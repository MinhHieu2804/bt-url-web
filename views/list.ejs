<!DOCTYPE html>
<html>

<head>
	<meta charset="utf-8">
</head>

<body>
	<div class="output"></div>
	<script>
		fetch("/api/students")
			.then(response => response.json())
			.then(data => {
				document.querySelector("div.output").innerHTML = JSON.stringify(data.data);
			})
	</script>
</body>

</html>